{
	"name": "@pinkcig/tap",
	"description": "Creates application from provided templates, based on my opinionated tastes",
	"version": "1.0.1",
	"author": "Faye Keller",
	"license": "GPL-3.0",
	"repository": "git@github.com:pinkcig/tap.git",
	"bin": "dist/index.js",
	"main": "dist/index.js",
	"files": [
		"dist/*.js",
		"dist/*.d.ts",
		"!dist/*.js.map",
		"!dist/*.d.ts.map",
		"LICENSE",
		"README.md"
	],
	"keywords": [
		"tap",
		"cli",
		"cli-app",
		"cli-template",
		"cli-template-generator",
		"templates",
		"templates-generator",
		"template"
	],
	"scripts": {
		"build": "tsup && tsc -b src",
		"clean": "shx rm -rf dist",
		"dev": "tsup --watch",
		"prepublishOnly": "yarn run clean && yarn run build"
	},
	"dependencies": {
		"fs-extra": "^10.0.0",
		"inquirer": "^8.2.0",
		"mustache": "^4.2.0"
	},
	"devDependencies": {
		"@sapphire/ts-config": "^3.3.0",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^8",
		"@types/mustache": "^4",
		"@types/node": "^17.0.14",
		"shx": "^0.3.4",
		"tsup": "^5.11.13",
		"typescript": "^4.5.5"
	},
	"packageManager": "yarn@3.1.1",
	"engines": {
		"node": "^16",
		"npm": "^7"
	}
}
